#include <stdio.h>

typedef unsigned char     uint8_t;
typedef unsigned short    uint16_t;
typedef unsigned long     uint32_t;

int main()
{
    unsigned int val = 976;
    
    uint16_t Right = (uint16_t)(val) & 0x00ff;
    uint16_t left = left | (val & 0xff00);
    
    
    printf("%d",val);
    printf("%d",Right);
    printf("%d",left);

    return 0;
}
